<Window x:Class="Aspirantes.Reporte"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Aspirantes"
        mc:Ignorable="d"
        Title="Reporte" Height="300" Width="908.786">
    <Grid Loaded="Grid_Loaded">
        <DataGrid x:Name="grid1" HorizontalAlignment="Left" Margin="30,54,0,0" VerticalAlignment="Top" Height="200" Width="848"/>
        <Label Content="Aspirantes con el programa" HorizontalAlignment="Left" Margin="183,7,0,0" VerticalAlignment="Top" FontSize="24"/>
        <ComboBox x:Name="cb_programa" HorizontalAlignment="Left" Margin="510,17,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="cb_programa_SelectionChanged"/>
        <Button x:Name="btn_regreso" Content="← " HorizontalAlignment="Left" Margin="19,10,0,0" VerticalAlignment="Top" Width="52" FontSize="22" Height="29" Click="btn_regreso_Click"/>

    </Grid>
</Window>
