<Window x:Class="Aspirantes.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LogIn" Height="300" Width="300">
    <Grid Loaded="Grid_Loaded_1">
        <Label Content="LogIn" HorizontalAlignment="Left" Margin="113,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Usuario" HorizontalAlignment="Left" Margin="10,44,0,0" VerticalAlignment="Top"/>
        <Label Content="Contraseña" HorizontalAlignment="Left" Margin="10,117,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tb_usuario" HorizontalAlignment="Left" Height="23" Margin="62,78,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tb_contraseña" HorizontalAlignment="Left" Height="23" Margin="62,148,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="btn_enviar" Content="Enviar" HorizontalAlignment="Left" Margin="182,212,0,0" VerticalAlignment="Top" Width="75" Click="btn_enviar_Click"/>

    </Grid>
</Window>
